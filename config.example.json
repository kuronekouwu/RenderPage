{
    "server": {
        "port": 8000
    },
    "puppeteer": {
        "headless": true,
        "useragent": "EXAMPLEAGENT / 1.0 (https://example.com)",
        "domain": [
            "https://example.com"
        ],
        "page": {
            "waitUntil": "networkidle2",
            "timeout": 30000,
            "maxFailures": 3
        }
    },
    "cache": {
        "path": "./cache",
        "maxAge": 259200
    }
}